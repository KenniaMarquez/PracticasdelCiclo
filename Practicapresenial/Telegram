const {Telegraf} = require("telegraf");
const bot = new Telegraf("5392433482:AAEYyZ_9XFeIDiIHEWxbu4g7z9yP9zWkY-0");

bot.start((ctx)=>{
    ctx.reply("Programacion ComputacionalIV");
});

